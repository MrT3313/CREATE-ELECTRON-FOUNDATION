{
  "name": "tp-reactr-tw",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "./dist-electron/main/index.js",
  "scripts": {
    "build": "vite build && electron-forge package",
    "build:mac:arm64": "vite build && electron-forge make --platform=darwin --arch=arm64 --verbose",
    "build:mac:x64": "vite build && electron-forge make --platform=darwin --arch=x64",
    "dev": "vite",
    "format": "prettier . --write",
    "lint": "eslint . --ext .ts,.tsx",
    "package": "electron-forge package",
    "tanstackrouter:generate:routes": "npx @tanstack/router-cli ./src/routes -y",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-form": "^1.14.1",
    "@tanstack/react-query": "~5.83.0",
    "classnames": "~2.5.1",
    "dayjs": "~1.11.13",
    "dotenv": "~16.5.0",
    "nanoid": "~5.1.5",
    "react": "~19.1.0",
    "react-dom": "~19.1.0",
    "react-router": "~7.6.2",
    "react-toastify": "~11.0.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron/rebuild": "4.0.1",
    "@tailwindcss/vite": "~4.1.11",
    "@tanstack/react-query-devtools": "~5.83.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "~4.6.0",
    "electron": "36.4.0",
    "electron-log": "5.4.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "tailwindcss": "~4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.0.0-alpha.54",
    "vite-plugin-electron": "~0.29.0"
  },
  "engines": {
    "node": "22.16.0"
  }
}
